! color
#define tc_black #0f0f0f
#define tc_white #efefef
#define tc_dark_gray #3f3f3f
#define tc_light_gray #bfbfbf
#define tc_dark_red #7f2f2f
#define tc_light_red #bf3f3f
#define tc_dark_green #5f8f2f
#define tc_light_green #7fbf3f
#define tc_dark_yellow #8f7f2f
#define tc_light_yellow #bf9f3f
#define tc_dark_blue #2f4f8f
#define tc_light_blue #3f5fbf
#define tc_dark_purple #8f2f7f
#define tc_light_purple #8f3f9f
#define tc_dark_cyan #2f8f8f
#define tc_light_cyan #3fbfbf
*foreground: #9f9f9f
*background: #0f1f2f
*color0: tc_black
*color1: tc_dark_red
*color2: tc_dark_green
*color3: tc_dark_yellow
*color4: tc_dark_blue
*color5: tc_dark_purple
*color6: tc_dark_cyan
*color7: tc_light_gray
*color8: tc_dark_gray
*color9: tc_light_red
*color10: tc_light_green
*color11: tc_light_yellow
*color12: tc_light_blue
*color13: tc_light_purple
*color14: tc_light_cyan
*color15: tc_white

! xterm
XTerm*cursorBlink: false
XTerm*forcePackedFont: false
XTerm*geometry: 80x24
XTerm*saveLines: 1024
XTerm*selectToClipboard: true
XTerm*visualBell: false
XTerm*wideChars: true
XTerm*utf8: 1
XTerm*locale: true
XTerm*utf8Title: true
!XTerm*faceName: Ricty:style=Regular
!XTerm*faceNameDoublesize: Ricty:style=Regular
XTerm*faceName: Inconsolata:style=Regular
XTerm*faceNameDoublesize: Migu 1M:style=Regular
XTerm*faceSize: 14
